{
    "name": "magdy-hakam/laravel-testids",
    "description": "Laravel package to add data-testid attribute to <a> and <input> tags in Blade files",
    "type": "library",
    "require": {
        "illuminate/console": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "magdy-hakam\\TestIds\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "magdy-hakam\\TestIds\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "App\\Console\\Commands"
            ]
        }
    }
}
